  "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact", "css", "scss", "Markdown"],
  "suggest.maxCompleteItemCount": 20,
  "tsserver.enableJavascript": true,
  "eslint.options": {"configFile": "~/.vim/.eslintrc.js"},
